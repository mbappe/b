#!/bin/sh -x

# Use a -DDEBUG build.

: \
&& b -W0 -1 -n1000000 -S1 --splay-key-bits=0xff00aa00aa00ffff \
&& bcheckall 64 \
&& b -W0 -1ICvdgi -n1000000 --splay-key-bits=0xff00aa00aa00ffff \
&& b -W0 -1ICvdgi -n1000000 -DS1 --splay-key-bits=0xff00aa00aa00ffff \
&& b -W0 -1ICvdgi -n1000000 \
&& b -W0 -1ICvdgi -n1000000 -S1 \
&& b -W0 -1ICvdgi -n1000000 -DS1 \
&& b -W0 -1ICvdgi -n1000000 -S1 --splay-key-bits=0xff00aa00aa00ffff \
&& b -W0 -1lmt \
&& b -W0 -RICvdgi -n1000000 \
:

